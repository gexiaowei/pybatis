<mapper>
    <select id="selectd">
        select * from tb_user
        <where prefixOverrides="AND">
            <if test="age>10"> age = ${age}</if>
        </where>
        order by id
    </select>
    <select id="select_user">
        select * from tb_user
        <where prefixOverrides="AND">
            <if test="age>10"> age = ${age}</if>
        </where>
        order by id
    </select>
    <select id="select_user_by_id">
        select * from ebt_user where ebf_user_id=${user_id}
    </select>
</mapper>